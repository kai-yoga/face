/*
该参数文件若不在当前路径下，则须指定该参数文件的路径
*/
{
  "bdzp": //比对照片的参数信息
  {
    "libs": {
      "file_key": "xh",
      //基础照片文件的命名规则,xh代表学号,zjhm代表证件号码
      "path": "C:\\Users\\80662\\Desktop\\libs"
      //基础照片文件的存放路径
    },
    "compare": {
      "suffix": [
        "jpg",
        "jpeg",
        "png"
      ],
      //比对照片支持的文件类型,可以添加
      "save_path": "",
      //结果文件的保存路径,为空表示当前路径
      "outputFileType": "xlsx"
      //结果文件的保存方式,xlsx或txt,不支持xls
    },
    "others": [
      {
        "file_key": "xh",
        //要比对的照片文件命名规则，xh表示学号，zjhm表示证件号码
        "path": "C:\\Users\\80662\\Desktop\\others",
        //要比对的照片文件存放路径
        "convert_file_path": "",
        /*
          要比对的照片文件若命名规则与基础照片文件的命名规则不一致，则需要指定
          两者之间的转换规则，此处即填写转换规则的文件路径,如D:\\转换.xlxs,
          转换文件必须是xlsx格式，而且第一行为：学号，证件号码
          */
        "similar": "1"
        /*指定要比对的照片文件与基础照片文件的最低相似度，若相似度低于该值
        则将结果保存到输出结果中
        */
      },
      //要比对的照片文件信息1


      {
        "file_key": "xh",
        "path": "",
        "convert_file_path": "",
        "similar": "0.7"
      }
      //要比对的文件信息2，参数信息与要比对的文件信息1相同

      /*
      如还有要比对的照片文件信息，则参照要比对的文件信息1，继续填写即可。
      */
    ]
  },
  "zip": //压缩照片的参数信息
  {
    "file_path": "",//要压缩的文件路径

    "save_path": "",    //压缩后的文件保存路径

    "save_file_suffix": [
      "jpg",
      "jpeg",
      "png"
    ],
    "max_count": "500",    //单个zip文件保存的最大文件数量,如500，则表示单个zip文件最多包含500个文件

    "max_storage": "50"    //单个文件保存的最大容量,M为单位，如50，则表示单个文件保存的最大容量为50M

    /* max_count与max_storage两个参数中一个先达到，即使另一个参数没有达到，也视为达到 */
  }
}